source .config/zsh/plugins/* # place plugin files (plugin.zsh) here
source .config/zsh/aliases.zsh # import aliases
source .config/zsh/bindkeys.zsh # import bindkeys
source .config/zsh/options.zsh # import zsh options
source .config/zsh/theme.zsh # import theme and styling

source .config/zsh/custom.zsh # Use this to add custom configs (file is .gitignored)

